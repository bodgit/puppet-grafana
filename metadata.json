{
  "name": "bodgit-grafana",
  "version": "1.0.0",
  "author": "bodgit",
  "summary": "Puppet Module for managing Grafana",
  "license": "Apache-2.0",
  "source": "https://github.com/bodgit/puppet-grafana",
  "project_page": "https://github.com/bodgit/puppet-grafana",
  "issues_url": "https://github.com/bodgit/puppet-grafana/issues",
  "operatingsystem_support": [
    {
